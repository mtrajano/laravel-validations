{
    "name": "mtrajano/laravel-validations",
    "description": "Common Laravel Validation Rules",
    "keywords": ["laravel-validations", "laravel","validation","rules"],
    "homepage": "https://github.com/mtrajano/laravel-validations",
    "license": "MIT",
    "authors": [
        {
            "name": "Mauricio Trajano",
            "email": "mauriciot1993@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Mtrajano\\LaravelValidations\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Mtrajano\\LaravelValidations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mtrajano\\LaravelValidations\\LaravelValidationsServiceProvider"
            ]
        }
    }
}
